            <Button fx:id="buttonRemove" mnemonicParsing="false" onAction="#onButtonRemoveClick" prefHeight="26.0" prefWidth="100.0" text="Remove" />
            <Button fx:id="buttonAbout" mnemonicParsing="false" onAction="#onButtonAboutClick" prefHeight="26.0" prefWidth="100.0" text="About" />
         </children>
      </HBox>
      <Label layoutX="294.0" layoutY="14.0" text="International Market Selection ">
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
      </Label>
      <TableView fx:id="tableView" layoutX="59.0" layoutY="55.0" prefHeight="278.0" prefWidth="762.0">
        <columns>
          <TableColumn fx:id="columnCountry" prefWidth="253.0" text="Country" />
          <TableColumn fx:id="columnMA" prefWidth="253.0" sortable="false" text="Market Attractiveness" />
            <TableColumn fx:id="columnCS" prefWidth="253.0" sortable="false" text="Competitive Strength" />
        </columns>
      </TableView>
      <ImageView fitHeight="200.0" fitWidth="200.0" layoutX="856.0" layoutY="94.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@vialogo.png" />
         </image>
      </ImageView>
      <Label layoutX="876.0" layoutY="279.0" prefHeight="30.0" prefWidth="160.0" text="Bring ideas to life" textAlignment="CENTER">
         <font>
            <Font name="Leelawadee UI" size="20.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
