<AnchorPane prefHeight="400.0" prefWidth="1100.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.sep3forsoeg5.HelloController">
   <children>
      <HBox layoutX="151.0" layoutY="347.0" prefHeight="38.0" prefWidth="577.0" spacing="60.0">
